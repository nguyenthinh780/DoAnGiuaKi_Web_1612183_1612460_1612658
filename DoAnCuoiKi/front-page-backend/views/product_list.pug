extends layout

block content
  h1= title

  ul
  each product in list_products
    .col-xs-0.col-sm-4.pitem
      li

        img(src= 'images/'+ product.image)
        a(href=product.url) #{product.product_name}
        |  (#{product.stall.stall_name})

  else
    li Không có tài khoản trong cơ sở dữ liệu.
